  "scripts": {
    "start-dev-old": "nodemon --trace-warnings --trace-uncaught index.js",
    "start-dev-old2": "nodemon -- --trace-warnings --trace-uncaught",
    "start-dev-old3": "copyfiles ./src/**/*.graphql ./src/**/*.pug ./ts-build && ts-node-dev --respawn --debug --transpile-only -r tsconfig-paths/register --files ./src/index.js --inspect --trace-warnings --trace-uncaught",
    "start-dev": "NODE_ENV=development ts-node-dev --transpile-only --respawn -r tsconfig-paths/register --watch ./src/**/*.pug,./src/**/*.graphql --trace-warnings --trace-uncaught -- ./src/index.js",
    "start": "NODE_ENV=production ts-node --transpile-only -r tsconfig-paths/register src/index.js",
    "watch": "tsc --watch",
    "test": "NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true mocha --parallel --require mochawesome/register --require ts-node/register --require tsconfig-paths/register --require test/setup.ts  --reporter mochawesome --trace-warnings",
    "test-server:multer": "nodemon test/multer-test-server.js",
    "test:mkdir": "npm test ./test/file.spec.js",
    "lint:fix": "eslint --fix .",
    "lint:fix2": "eslint --fix **/*.ts **/*.js"
  },
